{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"@components/*": ["src/components/*"],
			"@assets/*": ["assets/*"],
			"@types": ["types/*.d.ts"]
		}
	},
	"include": [
		"**/*.ts",
		"src/**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"**/*.d.ts",
	]
}
